[
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[recipe_engine::file]/resources/fileutil.py",
      "--json-output",
      "/path/to/tmp/json",
      "ensure-directory",
      "--mode",
      "0777",
      "[START_DIR]/gsutil"
    ],
    "infra_step": true,
    "name": "workdir"
  },
  {
    "cmd": [
      "vpython",
      "-u",
      "RECIPE_MODULE[recipe_engine::url]/resources/pycurl.py",
      "--url",
      "https://raw.githubusercontent.com/GoogleCloudPlatform/gsutil/master/VERSION",
      "--status-json",
      "/path/to/tmp/json",
      "--outfile",
      "/path/to/tmp/"
    ],
    "name": "version",
    "~followup_annotations": [
      "@@@STEP_LINK@gsutil '4.21'@https://storage.googleapis.com/pub/gsutil_4.21.tar.gz@@@"
    ]
  },
  {
    "cmd": [
      "cipd",
      "search",
      "infra/gsutil",
      "-tag",
      "version:4.21",
      "-json-output",
      "/path/to/tmp/json",
      "-service-account-json",
      "/creds/service_accounts/service-account-cipd-builder.json"
    ],
    "name": "cipd search infra/gsutil version:4.21",
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"result\": []@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "cmd": [
      "vpython",
      "-u",
      "RECIPE_MODULE[recipe_engine::url]/resources/pycurl.py",
      "--url",
      "https://storage.googleapis.com/pub/gsutil_4.21.tar.gz",
      "--status-json",
      "/path/to/tmp/json",
      "--outfile",
      "[START_DIR]/gsutil/gsutil_4.21.tar.gz"
    ],
    "name": "download"
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[infra::third_party_packages]/resources/archive_util.py",
      "[START_DIR]/gsutil/gsutil_4.21.tar.gz",
      "[START_DIR]/gsutil"
    ],
    "name": "extract"
  },
  {
    "cmd": [
      "cipd",
      "pkg-build",
      "-in",
      "[START_DIR]/gsutil/gsutil",
      "-name",
      "infra/gsutil",
      "-out",
      "[START_DIR]/gsutil/package.cipd",
      "-json-output",
      "/path/to/tmp/json",
      "-install-mode",
      "copy"
    ],
    "name": "build gsutil",
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"result\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"instance_id\": \"40-chars-fake-of-the-package-instance_id\", @@@",
      "@@@STEP_LOG_LINE@json.output@    \"package\": \"infra/gsutil\"@@@",
      "@@@STEP_LOG_LINE@json.output@  }@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "cmd": [
      "cipd",
      "pkg-register",
      "[START_DIR]/gsutil/package.cipd",
      "-json-output",
      "/path/to/tmp/json",
      "-service-account-json",
      "/creds/service_accounts/service-account-cipd-builder.json",
      "-ref",
      "latest",
      "-tag",
      "version:4.21"
    ],
    "name": "register infra/gsutil",
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"result\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"instance_id\": \"40-chars-fake-of-the-package-instance_id\", @@@",
      "@@@STEP_LOG_LINE@json.output@    \"package\": \"infra/gsutil\"@@@",
      "@@@STEP_LOG_LINE@json.output@  }@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "jsonResult": null,
    "name": "$result"
  }
]