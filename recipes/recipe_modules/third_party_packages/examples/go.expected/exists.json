[
  {
    "cmd": [
      "vpython",
      "-u",
      "RECIPE_MODULE[recipe_engine::file]/resources/fileutil.py",
      "--json-output",
      "/path/to/tmp/json",
      "ensure-directory",
      "--mode",
      "0777",
      "[START_DIR]/go_linux_32"
    ],
    "infra_step": true,
    "name": "workdir"
  },
  {
    "cmd": [
      "vpython",
      "-u",
      "RECIPE_MODULE[recipe_engine::url]/resources/pycurl.py",
      "--url",
      "https://golang.org/VERSION?m=text",
      "--status-json",
      "/path/to/tmp/json",
      "--outfile",
      "/path/to/tmp/"
    ],
    "name": "version",
    "~followup_annotations": [
      "@@@STEP_LINK@go '1.2.3'@https://storage.googleapis.com/golang/go1.2.3.linux-386.tar.gz@@@"
    ]
  },
  {
    "cmd": [
      "cipd",
      "search",
      "infra/go/linux-386",
      "-tag",
      "version:1.2.3",
      "-json-output",
      "/path/to/tmp/json",
      "-service-account-json",
      "/creds/service_accounts/service-account-cipd-builder.json"
    ],
    "name": "cipd search infra/go/linux-386 version:1.2.3",
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"result\": [@@@",
      "@@@STEP_LOG_LINE@json.output@    {@@@",
      "@@@STEP_LOG_LINE@json.output@      \"instance_id\": \"resolved-instance_id-of-instance_id_1---\", @@@",
      "@@@STEP_LOG_LINE@json.output@      \"package\": \"infra/go/linux-386\"@@@",
      "@@@STEP_LOG_LINE@json.output@    }@@@",
      "@@@STEP_LOG_LINE@json.output@  ]@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "cmd": [
      "python",
      "-u",
      "import sys; sys.exit(0)"
    ],
    "name": "Synced",
    "~followup_annotations": [
      "@@@STEP_TEXT@Package is up to date.@@@"
    ]
  },
  {
    "name": "$result"
  }
]