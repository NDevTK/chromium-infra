{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "experimentalDecorators": true,
    "inlineSources": true,
    "lib": ["es2017", "dom", "dom.iterable"],
    "module": "es2015",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": false,
    "sourceMap": true,
    "strict": true,
    "target": "es2017",
    "tsBuildInfoFile": ".tsbuildinfo",
    "types": ["chai", "mocha"],
  },
  "exclude": [
    "dist",
    "node_modules",
  ],
}
